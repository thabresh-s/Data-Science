        "    largest = item \n",
        "  elif largest2 == None or largest2 < item: \n",
        "    largest2 = item \n",
        "  if item < lowest: \n",
        "    lowest2 = lowest\n",
        "    lowest = item \n",
        "  elif lowest2 == None or lowest2 > item: \n",
        "    lowest2 = item \n",
        "              \n",
        "\n",
        "print(\"Smallest element is:\", lowest) \n",
        "\n",
        "print(\"Second Smallest element is:\", lowest2) "
